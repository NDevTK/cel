network {
  name: 'primary'
}

#######################################
# Remote Desktop Services in a Windows Server 2012 domain

ad_domain {
  name: 'domain2012.com'
  netbios_name: 'domain2012'

  domain_controller {
    windows_machine: 'win2012-dc'
  }
}

# Domain controller.
windows_machine {
  name: 'win2012-dc'
  machine_type: 'win2012r2'
  network_interface { network: 'primary' }
}

# Remote Desktop on Win2012 
windows_machine {
  name: 'win2012-rds'
  machine_type: 'win2012r2'
  network_interface { network: 'primary' }

  container { ad_domain: 'domain2012.com' }
}

remote_desktop_host {
  windows_machine: 'win2012-rds'
  collection_name: 'c1'
  collection_description: 'description'
}

# Remote Desktop on Win2016 
windows_machine {
  name: 'win2016-rds'
  machine_type: 'win2016'
  network_interface { network: 'primary' }

  container { ad_domain: 'domain2012.com' }
}

remote_desktop_host {
  windows_machine: 'win2016-rds'
  collection_name: 'c1'
  collection_description: 'description'
}